{
    "name": "Z0Z_-extensions-for-visual-studio-code",
    "displayName": "Z0Z_ Extensions for Visual Studio Code",
    "description": "code harder",
    "version": "0.0.2",
    "publisher": "hunterhogan",
    "author": {
        "name": "Hunter Hogan",
        "email": "HunterHogan@pm.me"
    },
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Other",
        "SCM Providers"
    ],
    "keywords": [
        "git",
        "exclude",
        "gitignore",
        "context menu",
        "source control"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/hunterhogan/Z0Z_VScode.git"
    },
    "bugs": {
        "url": "https://github.com/hunterhogan/Z0Z_VScode/issues"
    },
    "homepage": "https://github.com/hunterhogan/Z0Z_VScode",
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "Z0Z_-extensions-for-visual-studio-code.addToExclude",
                "title": "Add to .git/info/exclude"
            },
            {
                "command": "Z0Z_-extensions-for-visual-studio-code.toggleProblemsVisibility",
                "title": "Toggle Problems Visibility"
            }
        ],
        "menus": {
            "scm/resourceState/context": [
                {
                    "command": "Z0Z_-extensions-for-visual-studio-code.addToExclude",
                    "when": "scmProvider == git",
                    "group": "1_modification@4"
                }
            ]
        }
    }
}
